<Window x:Class="PracticalWork20.Search"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PracticalWork20"
        mc:Ignorable="d"
        Title="Поиск" Height="200" Width="300"
        WindowStartupLocation="CenterOwner">
    <StackPanel HorizontalAlignment="Center" Margin="20">
        <TextBlock HorizontalAlignment="Center">Введите фамилию клиента</TextBlock>
        <TextBox x:Name="searchText" Margin="20" Width="150"/>
        <Button x:Name="btnSearch" IsDefault="True" Content="Поиск" HorizontalAlignment="Center" Padding="20, 5" Click="Search_Click"/>
    </StackPanel>
</Window>
